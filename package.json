{
  "name": "deemoney-framer-widgets",
  "version": "0.1.0",
  "private": false,
  "description": "Reusable widgets for Framer (Free via CDN Embed, Paid via npm).",
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:lib": "tsup --config tsup.config.ts --env.NODE_ENV=production",
    "build:standalone": "tsup --config tsup.config.ts --env.NODE_ENV=production",
    "build": "npm run clean && tsup --config tsup.config.ts --env.NODE_ENV=production",
    "dev": "tsup --config tsup.config.ts --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=17 || ^18",
    "react-dom": ">=17 || ^18",
    "react-hook-form": ">=7"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "i18next": "^23.12.3",
    "react-i18next": "^15.5.1"
  }
}
